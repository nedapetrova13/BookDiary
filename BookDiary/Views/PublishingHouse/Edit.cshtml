@{
}
@model BookDiary.Models.ViewModels.PublishingHouseViewModels.PublishingHouseEditViewModel;
<form method="post">
    <label asp-for="Name">
        Name
        <input type="text" asp-for="Name" />
    </label>
    <span asp-validation-for="Name"></span>
    <br />
    <label asp-for="YearFounded">
        Year
        <input type="year" asp-for="YearFounded" />
    </label>
    <span asp-validation-for="YearFounded"></span>
    <br />
    <button type="submit" class="btn btn-primary" asp-action="Edit" asp-controller="PublishingHouse" class="btn btn-success" >Submit changes</button>
</form>