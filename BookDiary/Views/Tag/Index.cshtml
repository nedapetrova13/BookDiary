@{
}
@model IEnumerable<BookDiary.Models.Tag>
<table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var tag in Model){
            <tr>
                <th scope="row">@tag.Id</th>
                <th>@tag.Name</th>
            </tr>
        }
    </tbody>
</table>