@model IEnumerable<BookDiary.Models.Book>
@foreach (var book in Model)
{
    <div class="card" style="width: 18rem;">
        <img src="@book.CoverImageURL" class="card-img-top" alt="...">
        <div class="card-body">
            <h5 class="card-title">@book.Title</h5>
            <a type="button" class="btn btn-primary" asp-action="Edit" asp-controller="Book" asp-route-id="@book.Id">Edit</a>
        </div>
    </div>
}
